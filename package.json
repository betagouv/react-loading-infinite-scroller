    "prop-types": "^15.6.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-infinite-scroller": "^1.2.4",
    "with-react-query": "1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.22.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-15": "^1.1.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^6.3.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jest": "^22.7.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "jest": "^24.8.0",
    "jest-environment-jsdom": "^24.5.0",
    "nodemon": "^1.11.0",
    "prettier-eslint-cli": "^5.0.0",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.5.1",
    "redux": "^4.0.0"
  },
  "keywords": [
    "react",
    "loading",
    "infinite",
    "scroller"
  ],
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "name": "react-loading-infinite-scroller",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/betagouv/react-loading-infinite-scroller"
  },
  "scripts": {
    "compile": "rm -rf lib && babel --plugins transform-class-properties,transform-object-rest-spread --presets env,react -d lib/ src/",
    "postinstall": "if [ -d \"src/\" ]; then npm run compile; fi",
    "precommit": "./precommit.sh",
    "prettier": "$(yarn bin)/prettier --write 'src/**/*.js'",
    "prepublish": "npm run compile",
    "test": "yarn jest --env=jsdom ./src",
    "watch": "nodemon --watch src --exec \"npm run compile\""
  },
  "version": "1.3.1"
}
